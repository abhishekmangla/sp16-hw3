<h1>Home</h1>
<% @users.each do |user| %>
	<h2><%= user.username %></h2>
		<p><%= user.email %><p>
			<p><%= user.age %><p>
				<p><%= user.birthday %><p>

<% end %>

<% @cats.each do |cat| %>
<h2><%= cat.name %></h2>
	<p><%= cat.food %><p>

<% end %>



<% @todos.each do |todo| %>
<h2><%= todo.tasks %></h2>
<p><%= todo.thing %><p>
	<p><%= todo.finished %><p>


<% end %>